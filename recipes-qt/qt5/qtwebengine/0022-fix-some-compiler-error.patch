From 8d167d6feceecc94927e18147b47fe6cc3b84eb5 Mon Sep 17 00:00:00 2001
From: Qizheng Zhu <zhuqizheng@loongson.cn>
Date: Tue, 13 Jun 2023 08:25:00 +0000
Subject: [PATCH] fix some compiler error

---
 .../chromium/net/third_party/quiche/src/quic/core/quic_tag.h    | 1 +
 .../breakpad/src/client/linux/handler/exception_handler.cc      | 2 +-
 .../perfetto/src/trace_processor/containers/string_pool.h       | 1 +
 3 files changed, 3 insertions(+), 1 deletion(-)

diff --git a/src/3rdparty/chromium/net/third_party/quiche/src/quic/core/quic_tag.h b/src/3rdparty/chromium/net/third_party/quiche/src/quic/core/quic_tag.h
index 71fcdd597..958a5bde9 100644
--- a/src/3rdparty/chromium/net/third_party/quiche/src/quic/core/quic_tag.h
+++ b/src/3rdparty/chromium/net/third_party/quiche/src/quic/core/quic_tag.h
@@ -8,6 +8,7 @@
 #include <map>
 #include <string>
 #include <vector>
+#include <stdint.h>
 
 #include "net/third_party/quiche/src/quic/platform/api/quic_export.h"
 
diff --git a/src/3rdparty/chromium/third_party/breakpad/breakpad/src/client/linux/handler/exception_handler.cc b/src/3rdparty/chromium/third_party/breakpad/breakpad/src/client/linux/handler/exception_handler.cc
index a6b733875..5d76052c6 100644
--- a/src/3rdparty/chromium/third_party/breakpad/breakpad/src/client/linux/handler/exception_handler.cc
+++ b/src/3rdparty/chromium/third_party/breakpad/breakpad/src/client/linux/handler/exception_handler.cc
@@ -141,7 +141,7 @@ void InstallAlternateStackLocked() {
   // SIGSTKSZ may be too small to prevent the signal handlers from overrunning
   // the alternative stack. Ensure that the size of the alternative stack is
   // large enough.
-  static const unsigned kSigStackSize = std::max(16384, SIGSTKSZ);
+  static const unsigned kSigStackSize = std::max(16384, int(SIGSTKSZ));
 
   // Only set an alternative stack if there isn't already one, or if the current
   // one is too small.
diff --git a/src/3rdparty/chromium/third_party/perfetto/src/trace_processor/containers/string_pool.h b/src/3rdparty/chromium/third_party/perfetto/src/trace_processor/containers/string_pool.h
index 11ae91cfe..c2c2df5d7 100644
--- a/src/3rdparty/chromium/third_party/perfetto/src/trace_processor/containers/string_pool.h
+++ b/src/3rdparty/chromium/third_party/perfetto/src/trace_processor/containers/string_pool.h
@@ -19,6 +19,7 @@
 
 #include <stddef.h>
 #include <stdint.h>
+#include <limits>
 
 #include <unordered_map>
 #include <vector>
-- 
2.30.2

